<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
  <a href="/auth/facebook"><img alt="Sign in with Facebook" src="/images/omnisocial/signin_facebook.png" /></a> 
  <a href="/auth/twitter"><img alt="Sign in with Twitter" src="/images/omnisocial/signin_twitter.png" /></a> 
  <%= yield %>

</body>
</html>
